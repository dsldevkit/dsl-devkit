            <keywordReference id="com.avaloq.tools.ddk.check.ui.keyword_Check"/>
        </page>
        <page
            category="com.avaloq.tools.ddk.check.Check"
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
