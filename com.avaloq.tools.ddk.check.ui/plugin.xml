<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

    <extension
            point="org.eclipse.ui.editors">
        <editor
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="check"
            icon="icons/editor.gif"
            id="com.avaloq.tools.ddk.check.Check"
            matchingStrategy="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.xbase.ui.editor.JavaEditorInputMatcher"
            name="Check Editor">
        </editor>
    </extension>
    <extension
        point="org.eclipse.ui.handlers">
        <handler
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
            commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
            <activeWhen>
                <reference
                    definitionId="com.avaloq.tools.ddk.check.Check.Editor.opened">
                </reference>
            </activeWhen>
        </handler>
        <handler
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
            commandId="com.avaloq.tools.ddk.check.Check.validate">
         <activeWhen>
            <reference
                    definitionId="com.avaloq.tools.ddk.check.Check.Editor.opened">
            </reference>
         </activeWhen>
        </handler>
        <!-- copy qualified name -->
        <handler
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
            <activeWhen>
        <reference definitionId="com.avaloq.tools.ddk.check.Check.Editor.opened" />
            </activeWhen>
        </handler>
        <handler
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
            commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
            <activeWhen>
              <and>
                <reference definitionId="com.avaloq.tools.ddk.check.Check.XtextEditor.opened" />
                  <iterate>
            <adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
          </iterate>
        </and>
            </activeWhen>
        </handler>
    </extension>
    <extension point="org.eclipse.core.expressions.definitions">
        <definition id="com.avaloq.tools.ddk.check.Check.Editor.opened">
            <and>
                <reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
                        value="com.avaloq.tools.ddk.check.Check"
                        forcePluginActivation="true"/>
                </with>
            </and>
        </definition>
        <definition id="com.avaloq.tools.ddk.check.Check.XtextEditor.opened">
            <and>
                <reference definitionId="isXtextEditorActive"/>
                <with variable="activeEditor">
                    <test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
                        value="com.avaloq.tools.ddk.check.Check"
                        forcePluginActivation="true"/>
                </with>
            </and>
        </definition>
    </extension>
    <extension
            point="org.eclipse.ui.preferencePages">
        <page
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
            id="com.avaloq.tools.ddk.check.Check"
            name="Check">
            <keywordReference id="com.avaloq.tools.ddk.check.ui.keyword_Check"/>
        </page>
        <page
            category="com.avaloq.tools.ddk.check.Check"
            class="com.avaloq.tools.ddk.check.ui.CheckExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
