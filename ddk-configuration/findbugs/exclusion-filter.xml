<FindBugsFilter>
  <Match>
      <Bug code="OBL" />
      <Class name="~.*Service.*" />
  </Match>
  <Match>
      <Bug code="UwF, SIC, NP, EC" />
      <Class name="~.*Test.*" />
  </Match>
  <Match>
      <Bug code="UwF" />
      <Or>
        <Class name="~.*Action.*" />
        <Class name="~.*WizardPage.*" />
        <Class name="~.*View.*" />
      </Or>
   </Match>
  <Match>
     <Bug code="EI,EI2" />
  </Match>
  <Match>
     <Bug pattern="SE_COMPARATOR_SHOULD_BE_SERIALIZABLE" />
  </Match>
  <Match>
     <Bug pattern="URF_UNREAD_PUBLIC_OR_PROTECTED_FIELD" />
  </Match>
  <Match>
     <Bug pattern="SIC_INNER_SHOULD_BE_STATIC" />
     <Class name="com.avaloq.tools.ddk.caching.CacheManager$MultiCacheStatistics.*" />
  </Match>
  <Match>
     <Bug pattern="IC_SUPERCLASS_USES_SUBCLASS_DURING_INITIALIZATION" />
     <Class name="com.avaloq.tools.ddk.xtext.modelinference.ModelInferenceFactory.*" />
  </Match>
  <Match>
     <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT" />
  </Match>
  <Match>
     <Bug pattern="IMSE_DONT_CATCH_IMSE" />
  </Match>
  <Match>
     <Bug pattern="IS2_INCONSISTENT_SYNC" />
  </Match>
  <Match>
     <Bug pattern="MS_PKGPROTECT" />
     <Or>
       <Class name="~.*\.ModelInferenceSwitch.*" />
       <Class name="~.*\.ModelInferenceAdapterFactory.*" />
     </Or>
  </Match>
  <Match>
     <Bug code="Nm,EC" />
     <Class name="~.*\.generator\..*[Field|Type|Component]Util" />
  </Match>
  <Match>
     <Bug code="SIO" />
     <Class name="~.*\.generator\..*Generator" />
  </Match>
  <Match>
     <Bug code="EC,IC" />
     <Class name="~.*\.impl\..*Impl" />
  </Match>
</FindBugsFilter>
